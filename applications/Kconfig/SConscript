Import('env', 'lib', 'cocoexe')

targets = ['Scanner.h', 'Scanner.c', 'Parser.h', 'Parser.c']
env.Command(targets, ['Kconfig.atg'], '"%s" -g $SOURCES' % str(cocoexe[0]))
env.Depends(targets, cocoexe)
env.Program('TestKconfig', ['main.c', 'KcData.c',
                            'Parser.c', 'Scanner.c', lib])
